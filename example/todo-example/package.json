{
  "name": "todo-example",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' 'cypress/**/*.js' '*.config.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' 'cypress/**/*.js' '*.config.{js,ts}' --fix",
    "fmt": "prettier --check 'src/**/*.{js,jsx,ts,tsx}' 'cypress/**/*.js' '*.config.{js,ts}'",
    "fmt:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx}' 'cypress/**/*.js' '*.config.{js,ts}'",
    "check": "npm run lint && npm run fmt",
    "check:fix": "npm run lint:fix && npm run fmt:fix",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "cypress:lint": "eslint 'cypress/**/*.js'",
    "cypress:format": "prettier --write 'cypress/**/*.js'",
    "cypress:clean": "rm -rf cypress/pacts cypress/screenshots cypress/videos",
    "http-server": "http-server -p 3000 build/ &"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@pactflow/pact-cypress-adapter": "1.3.0",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "cypress": "15.9.0",
    "eslint": "9.39.2",
    "eslint-plugin-cypress": "5.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "http-server": "14.1.1",
    "prettier": "3.8.1",
    "tsx": "^4.19.2",
    "vite": "^7.3.1"
  }
}
